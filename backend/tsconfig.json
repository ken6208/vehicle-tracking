{
    "compilerOptions": {
      "target": "ES2019",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "typeRoots": ["./node_modules/@types", "src/typesss"]
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  